{
    "name": "ob-ivan/diversitest",
    "description": "Run tests against varying dependencies versions",
    "config": {
        "sort-packages": true
    },
    "require": {
        "symfony/console": "^3.0|^4.0",
        "symfony/process": "^3.0|^4.0|^5.0",
        "symfony/yaml": "^3.0|^4.0",
        "twig/twig": "^1.41|^2.7"
    },
    "autoload": {
        "psr-4": {
            "Ob_Ivan\\DiversiTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "bin": [
        "diversitest"
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    }
}
